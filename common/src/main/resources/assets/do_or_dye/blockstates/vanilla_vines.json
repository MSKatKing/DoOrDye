{
  "multipart": [
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines"
      },
      "when": {
        "AND": [
          { "north": "true" },
          { "height": "1|2|3|4|5|6|7" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "AND": [
          { "east": "true" },
          { "height": "1|2|3|4|5|6|7" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "AND": [
          { "south": "true" },
          { "height": "1|2|3|4|5|6|7" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "AND": [
          { "west": "true" },
          { "height": "1|2|3|4|5|6|7" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines_half"
      },
      "when": {
        "AND": [
          { "north": "true" },
          { "height": "8" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines_half",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "AND": [
          { "east": "true" },
          { "height": "8" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines_half",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "AND": [
          { "south": "true" },
          { "height": "8" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines_half",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "AND": [
          { "west": "true" },
          { "height": "8" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/empty"
      },
      "when": {
        "OR": [
          { "up": false },
          { "north": false },
          { "south": false },
          { "east": false },
          { "west": false }
        ]
      }
    }
  ]
}