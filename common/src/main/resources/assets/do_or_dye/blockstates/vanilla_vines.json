{
  "multipart": [
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines"
      },
      "when": {
        "AND": [
          { "direction": "north" },
          { "height": "1|2|3|4|5|6|7" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "AND": [
          { "direction": "east" },
          { "height": "1|2|3|4|5|6|7" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "AND": [
          { "direction": "south" },
          { "height": "1|2|3|4|5|6|7" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "AND": [
          { "direction": "west" },
          { "height": "1|2|3|4|5|6|7" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines_half"
      },
      "when": {
        "AND": [
          { "direction": "north" },
          { "height": "8" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines_half",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "AND": [
          { "direction": "east" },
          { "height": "8" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines_half",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "AND": [
          { "direction": "south" },
          { "height": "8" }
        ]
      }
    },
    {
      "apply": {
        "model": "do_or_dye:block/vanilla_vines_half",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "AND": [
          { "direction": "west" },
          { "height": "8" }
        ]
      }
    }
  ]
}